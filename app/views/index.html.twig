{% extends "base.html.twig" %}

{% block body %}

<table class="table table-striped table-nonfluid">
	<tr><th>Report</th><th></th><th></th></tr>
	{% for report in reports %}
		<tr><td>{{ report }}</td><td><a href="{{ url('html', {rep: report}) }}">HTML</a></td><td><a href="{{ url('excel', {rep: report}) }}">EXCEL</a></td></tr>
	{% endfor %}
</table>

{% endblock %}